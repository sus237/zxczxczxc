local OrigYellow = Color3.new(255/255, 255/255, 0);

local SFrame;

--ButtonSingle, ButtonDouble, ColorPicker, TextButton

function AddCommands(theTable,...)
	local args = {...}
	for i, v in pairs(args) do
		table.insert(theTable, v)
	end
end

